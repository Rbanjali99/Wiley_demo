package JavaThreads;

class Sheet{
	public void writeln(int k) {
		for (int i = 1 ; i<= 10; i++) {
			System.out.println(i*k);
		}
	}
}

class Person1 extends Thread{
	Sheet st;
	public Person1(Sheet st) {
		super();
		this.st = st;
	}
	public void run() {
		st.writeln(10);
	}
}

public class SynchronizationConcepts {

	

}

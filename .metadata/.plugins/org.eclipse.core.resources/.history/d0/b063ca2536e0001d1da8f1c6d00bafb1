package Core_java;

class Animal{
	public void eat() {
		System.out.println("eat");
	}
	public void sleep() {
		System.out.println("sleep");
	}
}

class Lion extends Animal{
	public void eat() {
		System.out.println("eating lion");
	}
	public void roar() {
		System.out.println("roar");
	}
}
